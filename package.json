{
  "name": "u-shows",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "24.13.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "types": "vue-tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,vue}\""
  },
  "dependencies": {
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.8.0",
    "glob": "^13.0.6",
    "jsdom": "^28.1.0",
    "minimatch": "^10.2.2",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.5"
  },
  "//overrides": "Forces all nested dependencies to use secure versions of minimatch and glob. Fixes CVE-2024-4067 (ReDoS vulnerability in minimatch <10.2.1) in transitive dependencies from @vue/test-utils, typescript-eslint, and other dev tools.",
  "overrides": {
    "minimatch": "^10.2.2",
    "glob": "^13.0.6"
  }
}
